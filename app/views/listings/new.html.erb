<h1>Create new listing</h1>

<%= form_for :listing, url: listings_path do |f| %>
  <p>
    <%= f.label :address %><br>
    <%= f.text_field :address %>
  </p>
  
  <p>
    <%= f.label :start_time %><br>
    <%= f.datetime_select :start_time %>
  </p>

  <p>
    <%= f.label :end_time %><br>
    <%= f.datetime_select :end_time %>
  </p>
 
  <p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
